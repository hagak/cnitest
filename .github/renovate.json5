{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    "docker:enableMajor",
    "replacements:k8s-registry-move",
    ":automergeBranch",
    ":automergeDigest",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":gitSignOff",
    ":semanticCommits",
    ":skipStatusChecks",
    ":timezone(America/New_York)",
    "github>hagak/renovate//allowedVersions.json5",
    "github>hagak/renovate//autoMerge.json5",
    "github>hagak/renovate//commitMessage.json5",
    "github>hagak/renovate//customManagers.json5",
    "github>hagak/renovate//grafanaDashboards.json5",
    "github>hagak/renovate//groups.json5",
    "github>hagak/renovate//labels.json5",
    "github>hagak/renovate//packageRules.json5",
    "github>hagak/renovate//semanticCommits.json5",
  ],
  dependencyDashboardTitle: "Renovate Dashboard ðŸ¤–",
  suppressNotifications: ["prEditedNotification", "prIgnoreNotification"],
  rebaseWhen: "conflicted",
  assigneesFromCodeOwners: true,
  reviewersFromCodeOwners: false,
  ignorePaths: ["**/*.sops.*", "**/resources/**"],
  flux: {
    fileMatch: [
        "(^|/)kubernetes/.+\\.ya?ml$"
    ],
  },
  "helm-values": {
    fileMatch: [
        "(^|/)kubernetes/.+\\.ya?ml$"
    ],
  },
  kubernetes: {
    fileMatch: [
        "(^|/)\\.taskfiles/.+\\.ya?ml$",
        "(^|/)kubernetes/.+\\.ya?ml$"
    ],
  },
}
